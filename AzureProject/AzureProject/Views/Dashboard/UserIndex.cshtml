<head>
    <style>
    body {
    background-image: url('data:image/jpeg;base64,');
    background-repeat: no-repeat;
    background-size:cover;
    font-size:large;
    table, th, td {
  border:solid black;
    }
    </style>
</head>
<body>
    <h1>Products View</h1>
    <br />
<div class="container">
    <table class="table table-bordered table-hover">
        <tr class="bg-success text-white">
            <th>ProductId</th>
            <th>Name</th>
            <th>Category</th>
            <th>Price</th>
            <th>Actions</th>
        </tr>
        @{
            try
            {
                if(TempData["Products"]!=null)
                {
                    foreach(var x in (List<AzureProject.Models.Products>)TempData["Products"])
                    {
                        <tr class=" table-light">
                        <td>@x.ProductId</td>
                        <td>@x.Name</td>
                        <td>@x.Category</td>
                        <td>@x.Price</td>
                        <td>
                         <a href="~/Dashboard/AddToCart?id=@x.ProductId" class="btn btn-warning">Add To Cart</a>

                        </td>
                        </tr>
                    }
                }
            }
            catch
            {
                
            }
        }
    </table>

</div>
</body>

